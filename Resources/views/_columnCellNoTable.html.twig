{# @KilikTable/_columnCell.html.twig #}
{% if not column.hidden %}
    <div {% if column.displayClass %}class="{{ column.displayClass }}"{% endif %}>
        {% set cellHtml=table.value(column,row) %}
        {% if column.raw %}
            {{ cellHtml | raw }}
        {% else %}
            {{ cellHtml }}
        {% endif %}
    </div>
{% endif %}
